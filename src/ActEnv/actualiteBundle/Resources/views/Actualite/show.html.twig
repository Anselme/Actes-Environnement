<h1>Actualite</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Title</th>                <td>{{ entity.title }}</td>        </tr>
        <tr>
            <th>Text</th>                <td>{{ entity.text }}</td>        </tr>
        <tr>
            <th>Isonline</th>                <td>{{ entity.isOnLine }}</td>        </tr>
        <tr>
            <th>Publicationdate</th>                <td>{{ entity.publicationDate|date('Y-m-d H:i:s') }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('actualite') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('actualite_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('actualite_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
